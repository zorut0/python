n=int(input('Enter amount: '))
c1=0
c10=0
c50=0
c100=0
c500=0
for i in range(1,n+1):
    if(i%1==0):
        c1=c1+1
    if(i%10==0):
        c10=c10+1
    if(i%50==0):
        c50=c50+1
    if(i%100==0):
        c100=c100+1
    if(i%500==0):
        c500=c500+1
print('No of 1 rupee in',n,':',c1)
print('No of 10 rupee in',n,':',c10)
print('No of 50 rupee in',n,':',c50)
print('No of 100 rupee in',n,':',c100)
print('No of 500 rupee in',n,':',c500)
